{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "script-social": {
      "root": "",
      "sourceRoot": "",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "src/main/resources/templates/angular_build",
            "index": "angular/index.html",
            "main": "angular/main.ts",
            "polyfills": "angular/polyfills.ts",
            "tsConfig": "tsconfig.json",
            "aot": false,
            "assets": [
              {
                "glob": "**/*",
                "input": "angular/assets",
                "output": "/assets/"
              }
            ],
            "styles": [
              "angular/components/app/app.component.css",
              "angular/components/elements/auth-bubble/auth-bubble.component.css",
              "angular/components/elements/auth-type-selector/auth-type-selector.component.css",
              "angular/components/elements/blinking-cursor/blinking-cursor.component.css",
              "angular/components/elements/connections-list-bubble/connections-list-bubble.component.css",
              "angular/components/elements/connections-list-button/connections-list-button.component.css",
              "angular/components/elements/console/console.component.css",
              "angular/components/elements/console-input/console-input.component.css",
              "angular/components/elements/console-popup/console-popup.component.css",
              "angular/components/elements/console-textbox/console-textbox.component.css",
              "angular/components/elements/content-link/content-link.component.css",
              "angular/components/elements/fade-in-content/fade-in-content.component.css",
              "angular/components/elements/general-post/general-post.component.css",
              "angular/components/elements/general-post-button/general-post-button.component.css",
              "angular/components/elements/general-post-buttons/general-post-buttons.component.css",
              "angular/components/elements/general-post-comment/general-post-comment.component.css",
              "angular/components/elements/general-post-comment-input/general-post-comment-input.component.css",
              "angular/components/elements/general-post-footer/general-post-footer.component.css",
              "angular/components/elements/general-post-header/general-post-header.component.css",
              "angular/components/elements/headline-button/headline-button.component.css",
              "angular/components/elements/message-bubble/message-bubble.component.css",
              "angular/components/elements/message-console/message-console.component.css",
              "angular/components/elements/message-console-connections-section/message-console-connections-section.component.css",
              "angular/components/elements/message-console-messages-section/message-console-messages-section.component.css",
              "angular/components/elements/messages-connections-list/messages-connections-list.component.css",
              "angular/components/elements/messages-input/messages-input.component.css",
              "angular/components/elements/messages-list/messages-list.component.css",
              "angular/components/elements/messages-section-title/messages-section-title.component.css",
              "angular/components/elements/nav-bar/nav-bar.component.css",
              "angular/components/elements/option-bubble/option-bubble.component.css",
              "angular/components/elements/options-list/options-list.component.css",
              "angular/components/elements/post-input-console/post-input-console.component.css",
              "angular/components/elements/profile-activity/profile-activity.component.css",
              "angular/components/elements/profile-bio/profile-bio.component.css",
              "angular/components/elements/profile-headline-buttons/profile-headline-buttons.component.css",
              "angular/components/elements/profile-name/profile-name.component.css",
              "angular/components/elements/profile-picture/profile-picture.component.css",
              "angular/components/elements/side-nav-bar/side-nav-bar.component.css",
              "angular/components/elements/side-nav-bar-button/side-nav-bar-button.component.css",
              "angular/components/elements/ss-anchor/ss-anchor.component.css",
              "angular/components/elements/ss-button/ss-button.component.css",
              "angular/components/elements/ss-code/ss-code.component.css",
              "angular/components/elements/ss-footer/ss-footer.component.css",
              "angular/components/elements/ss-img/ss-img.component.css",
              "angular/components/elements/typing-text/typing-text.component.css",
              "angular/components/elements/user-choice-bubble/user-choice-bubble.component.css",
              "angular/components/elements/user-choice-bubble-buttons/user-choice-bubble-buttons.component.css",
              "angular/components/elements/welcome-banner/welcome-banner.component.css",
              "angular/components/pages/authorize/authorize.component.css",
              "angular/components/pages/home/home.component.css",
              "angular/components/pages/messages/messages.component.css",
              "angular/components/pages/profile/profile.component.css",
              "angular/components/pages/welcome/welcome.component.css"
            ],
            "scripts": [],
            "fileReplacements": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "angular/environments/environment.ts",
                  "with": "angular/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "none",
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": true,
                  "inlineCritical": false
                }
              },
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            },
            "development": {
              "fileReplacements": [
                {
                  "replace": "angular/environments/environment.ts",
                  "with": "angular/environments/environment.dev.ts"
                }
              ],
              "optimization": false,
              "outputHashing": "none",
              "sourceMap": true,
              "namedChunks": true,
              "aot": false,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": false
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "script-social:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "script-social:build:production"
            },
            "development": {
              "buildTarget": "script-social:build:development"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "script-social:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "angular/test.ts",
            "polyfills": "angular/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "angular/assets"
            ],
            "styles": [
              "angular/styles/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "script-social:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "script-social:serve:production"
            },
            "development": {
              "devServerTarget": "script-social:serve:development"
            }
          }
        }
      }
    }
  }
}